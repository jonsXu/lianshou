        // // 生成的代码中，如果无法映射回源代码，就自动单步执行
        // "smartStep": true,  

        // // 指出编译后的文件地址
        // "outFiles": [
        //     "${workspaceRoot}/buildDebug/view/foods/index.js"
        // ]
        "name": "Launch",
        "type": "node",
        "request": "launch",
        "program": "${workspaceRoot}/src/view/foods/index.js",
        "stopOnEntry": false,
        "args": [],
        "cwd": "${workspaceRoot}",
        "preLaunchTask": "debug",
        "runtimeExecutable": null,
        "runtimeArgs": [ "--nolazy" ],
        "env": {
            "NODE_ENV": "development"
        },
        "externalConsole": false,
        "sourceMaps": true,
        "outDir": "${workspaceRoot}/buildDebug"
        },
    {
        "type": "node",
        "request": "attach",
        "name": "附加到端口",
        "address": "localhost",
        "port": 5858
      }
    ]
}